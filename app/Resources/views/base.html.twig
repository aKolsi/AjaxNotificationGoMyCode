{% block js %}
    <script>
        jQuery(document).ready(function() {
            Main.init();
            Index.init();
            FormElements.init();
        });

        setInterval(function(){
            $.ajax({
                url: "{{ path('act_nbr_notif') }}",
                type: 'POST',
                cache: false,
            }).done(function (response) {


                $('#nbrnotif').html(response);


            });
        },3000)

        function actualiserNbrNotification() {
            $.ajax({
                url: "{{ path('act_nbr_notifs') }}",
                type: 'POST',
                cache: false,
            }).done(function (response) {

                $('#nbrnotif').html(response);


            });


        }


        setInterval(function(){
            $.ajax({
                url: "{{ path('act_nbr_drop') }}",
                type: 'POST',
                cache: false,
            }).done(function (response) {


